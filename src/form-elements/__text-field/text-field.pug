mixin new-text-field(input_id, input_classes, label_classes, text, label_title) 
    div(class='text-field__wrapper')
        label(class=label_classes for=input_id)=label_title
        input(type="text" class=input_classes id=input_id)
        


link(rel="stylesheet" href="../src/form-elements/__text-field/text-field.css") 
+new-text-field("email","text-field", "text-field__label", "text-field__text", "Email") 

 

     
